{
  "name": "@suyanksaraswat/ui",
  "version": "9.0.0",
  "description": "An accessible and customizable component library powered by shadcn/ui",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/suyanksaraswat/ui",
    "directory": "packages/ui"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "ui",
    "component",
    "library",
    "design",
    "system",
    "accessible",
    "a11y",
    "radix",
    "shadcn"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "dev": "rollup -c --watch --bundleConfigAsCjs",
    "lint": "eslint src --fix --max-warnings 0",
    "format": "prettier --write src",
    "clean": "rimraf .turbo node_modules dist",
    "ui": "pnpm dlx shadcn@latest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.0.3",
    "@tanstack/react-table": "^8.10.7",
    "class-variance-authority": "^0.7.1",
    "react-hook-form": "^7.47.0",
    "sonner": "^1.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@suyanksaraswat/styles": "workspace:*",
    "@suyanksaraswat/tsconfig": "workspace:*",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "autoprefixer": "^10.4.16",
    "fs": "0.0.1-security",
    "lucide-react": "^0.288.0",
    "path": "^0.12.7",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "rollup": "^4.50.1",
    "rollup-plugin-dts": "^6.2.3",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.2",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@suyanksaraswat/styles": "*",
    "lucide-react": "*",
    "react": "^18",
    "zod": "^3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
